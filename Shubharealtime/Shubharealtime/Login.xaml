<Window x:Class="Shubharealtime.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Shubha Real Time" Height="260" Width="715" Loaded="Window_Loaded" Background="#FFE9E9F5" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Shubha Downloader " FontSize="15" Foreground="SteelBlue" Height="28" HorizontalAlignment="Left" Margin="81,10,0,0" Name="label1" VerticalAlignment="Top" Visibility="Hidden" />
        <Label Content="User Name :" Foreground="SteelBlue" Height="28" HorizontalAlignment="Left" Margin="29,67,0,0" Name="label2" VerticalAlignment="Top" Visibility="Hidden" />
        <Label Content="Password :" Foreground="SteelBlue" Height="28" HorizontalAlignment="Left" Margin="37,118,0,0" Name="label3" VerticalAlignment="Top" Visibility="Hidden" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="105,67,0,0" Name="username" TabIndex="1" VerticalAlignment="Top" Width="180" Visibility="Hidden" />
        <Button Content="Login" Height="23" HorizontalAlignment="Left" Margin="135,177,0,0" Name="Login_btn" TabIndex="3" VerticalAlignment="Top" Width="75" Click="Login_btn_Click" Visibility="Hidden" />
        <Button Content="cancel" Height="23" HorizontalAlignment="Left" Margin="223,172,0,0" Name="Cancle_btn" TabIndex="5" VerticalAlignment="Top" Width="75" Click="Cancle_btn_Click" Visibility="Hidden" />
        <Button Content="Register" Height="23" HorizontalAlignment="Left" Margin="37,177,0,0" Name="Regiser_btn" TabIndex="4" VerticalAlignment="Top" Width="75" Click="Regiser_btn_Click" Visibility="Hidden" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="106,124,0,0" Name="password" TabIndex="2" VerticalAlignment="Top" Width="180" Visibility="Hidden" />
        <Border BorderBrush="Silver" BorderThickness="2" Height="17" HorizontalAlignment="Left" Margin="10,44,0,0" Name="border1" VerticalAlignment="Top" Width="308" Visibility="Hidden"></Border>
        <WebBrowser Name="lead"  ScrollViewer.VerticalScrollBarVisibility="Hidden" ScrollViewer.IsDeferredScrollingEnabled="False" Width="715" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="3,-1,-15,2" Loaded="lead_Loaded" Navigated="lead_Navigated"></WebBrowser>

    </Grid>
</Window>
